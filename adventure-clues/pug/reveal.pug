						img.embed-responsive-item(src="http://placehold.it/500x500", alt="")
					
			

	
	.section
			
		.container

			.subsection
				
				+sectionTitle('did you know?')

				.text-shape.text-shape--green

					.text-shape__content

						p Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut 


	.section
			
		.container

			.subsection

				.text-center

					a.btn.btn-primary(href="#0") CONTINUE





				
						







